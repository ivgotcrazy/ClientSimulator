<?xml version="1.0" encoding="UTF-8" ?>
<root>
    <!--前置服务器地址-->
    <FrontAddr>TCP:192.168.5.185:1089</FrontAddr>
    <!--会议服务AppID-->
    <RoomAppID>11</RoomAppID>
    <!--前置服务AppID-->
    <FrontAppID>20</FrontAppID>
    <!--产品ID-->
    <ProductID>FMS01</ProductID>
    <!--客户端版本-->
    <ClientVersion>03.09.01.00</ClientVersion>
    <!--1:全功能，2:桌面合成，3:手机最小模式-->
    <ClientMode>1</ClientMode>
    <!--1:视频会议，2:IM，3:G3-->
    <ClientType>1</ClientType>
    <!--1:桌面端，2:硬件端，3:移动端，4:电话，5:录制-->
    <TerminalType>1</TerminalType>
    <!--要登录的会议室，一次只能登录一个会议室-->
    <RoomID>76824</RoomID>
    <!--会议室密码，没有密码则置空-->
    <RoomPwd>123456</RoomPwd>
    <!--用户视频设备个数，最大为6，User中配置优先级更高-->
    <VideoDevCount>3</VideoDevCount>
    <!--是否接收远端音视频数据-->
    <IfRecvAvData>0</IfRecvAvData>
    <!--用户登录速率控制-->
    <LoginParam>
        <ConcurrentLoginCount>1</ConcurrentLoginCount>
        <LoginCountPerSecond>1</LoginCountPerSecond>
    </LoginParam>
    <!--音频参数-->
    <AudioParam>
        <AudioDevIndex>1</AudioDevIndex>
    </AudioParam>
    <!--视频参数-->
    <VideoParam>
        <VideoDevIndex>0</VideoDevIndex>
        <Width>640</Width>
        <Height>480</Height>
        <FrameRate>60</FrameRate>
        <BitRate>8000000</BitRate>
        <EncodeMode>2</EncodeMode>
    </VideoParam>
    <!--UserName配置的是注册用户，NickName配置的是匿名用户-->
    <UserList>
        <User>
            <UserName>fm05</UserName>
            <UserPwd>000000</UserPwd>
            <VideoDevCount>6</VideoDevCount>
        </User>
    </UserList>
