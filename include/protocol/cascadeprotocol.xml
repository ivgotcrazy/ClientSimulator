<?xml version="1.0" encoding="UTF-8"?>
<root>
	<!--向上级服务器申请建立级联通道-->
	<cmd id="6800">
		<!--组所属节点ID-->
		<NodeID var="mcu1" /> 
		<!--组ID-->
		<GroupID val="10000"/>
		<!--申请节点ID-->
		<SrcNodeID val="mcu4"/>
		<!--级联验证码-->
		<AuthCode val="241324124"/>
		<ExData>
		</ExData>
	</cmd> 
	
	<!--服务器对下级申请建立级联通道的回应-->
	<cmd id="6801"> 
		<!--组所属节点ID-->
		<NodeID var="mcu1" /> 
		<!--组ID-->
		<GroupID val="10000"/>
		<!--级联结果-->
		<Result val="0"/>
		<ExData>
		</ExData>
	</cmd>
	
	<!--向上级服务器报告路由信息-->
	<cmd id="6802"> 
		<!--组所属节点ID-->
		<NodeID var="mcu1" /> 
		<!--组ID-->
		<GroupID val="10000"/>
		<!--节点ID-->
		<SrcNodeID val="mcu4"/>
		<!--节点路由信息-->
		<NodeRoute var="mcu2#mcu3#mcu4"/>
	</cmd>
	
	<!--服务器之间用级联通道转发数据-->
	<cmd id="6803"> 
		<!--组所属节点ID-->
		<NodeID var="mcu1" /> 
		<!--组ID-->
		<GroupID val="10000"/>
		<!--数据发送节点ID-->
		<SrcNodeID val="NodeID1"/>
		<ExData>
		</ExData>
		<!--转发的信令内容-->
		<cmd>
		</cmd>
	</cmd>
</root>